{% extends 'base.html.twig' %}

{% block title 'Contact' %}

{% block meta %}
    <meta name="turbo-cache-control" content="no-preview">
{% endblock %}

{% block body %}
    <h1>Contact</h1>

    <div id="contactdiv">
    {{ form_start(form) }}
        {{ form_row(form.name, { 
            'attr': { 'placeholder' : 'Insert name' }, 
            }) }}
        {{ form_row(form.message, { 'attr': { 'placeholder' : 'Insert message' }}) }}
        <button type="submit" formnovalidate>Send</button>
    {{ form_end(form) }}
    </div>
{% endblock %}